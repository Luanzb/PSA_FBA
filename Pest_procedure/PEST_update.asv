function [info,PEST] = PEST_update(info,PEST,t,sub,outcome)


    if info.first_row(trls.matrix(t,2),sub.ses_num) == t
        PEST(trls.matrix(t,2)) = PAL_AMRF_updateRF(PEST(trls.matrix(t,2), info.targ_alpha(t), outcome);
    else
        PEST(trls.matrix(t,2)) = PAL_AMRF_updateRF(PEST(update), PEST(update).xCurrent, outcome);
    end


end